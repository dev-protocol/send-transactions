{
	"name": "template-repos-ts",
	"version": "1.0.0",
	"type": "module",
	"description": "Template repository for using TypeScript",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"test": "vitest",
		"test:ci": "vitest --run",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,json,md,yml}'",
		"typecheck": "astro check && tsc --noEmit",
		"prepare": "husky install"
	},
	"author": "Dev Protocol",
	"license": "MPL-2.0",
	"dependencies": {
		"@astrojs/vercel": "^7.0.0",
		"@devprotocol/dev-kit": "8.6.1",
		"@devprotocol/util-ts": "4.0.0",
		"astro": "^4.0.0",
		"bignumber.js": "^9.1.2",
		"cross-fetch": "^4.0.0",
		"ethers": "^6.7.1",
		"p-retry": "^6.2.0",
		"ramda": "0.30.1",
		"redis": "^4.6.8"
	},
	"devDependencies": {
		"@astrojs/check": "^0.7.0",
		"@types/dotenv": "8.2.0",
		"@types/node": "20.14.6",
		"@types/ramda": "0.30.0",
		"@typescript-eslint/eslint-plugin": "7.13.1",
		"@typescript-eslint/parser": "7.13.1",
		"@vercel/node": "^3.0.0",
		"dotenv": "16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-functional": "6.6.0",
		"husky": "9.0.11",
		"prettier": "3.3.2",
		"rimraf": "5.0.7",
		"type-fest": "^4.10.2",
		"typescript": "5.5.2",
		"vitest": "^1.2.2"
	},
	"repository": "git+https://github.com/dev-protocol/template-repos-ts.git",
	"bugs": {
		"url": "https://github.com/dev-protocol/template-repos-ts/issues"
	},
	"homepage": "https://github.com/dev-protocol/template-repos-ts#readme",
	"private": true
}
